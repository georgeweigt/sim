; eor (zp,x)

	lda	#mem
	sta	zp+1
	lda	#mem>>8
	sta	zp+2
	lda	#3
	sta	mem
	lda	#5
	ldx	#1
	eor	(zp,x)
	cmp	#6
	beq	$+5
	jmp	fail

; eor zp

	lda	#3
	sta	zp
	lda	#5
	eor	zp
	cmp	#6
	beq	$+5
	jmp	fail

; eor #imm

	lda	#3
	eor	#5
	cmp	#6
	beq	$+5
	jmp	fail

; eor abs

	lda	#3
	sta	mem
	lda	#5
	eor	mem
	cmp	#6
	beq	$+5
	jmp	fail

; eor (zp),y

	lda	#mem
	sta	zp
	lda	#mem>>8
	sta	zp+1
	lda	#3
	sta	mem+1
	ldy	#1
	lda	#5
	eor	(zp),y
	cmp	#6
	beq	$+5
	jmp	fail

; eor zp,x

	lda	#3
	sta	zp+2
	ldx	#2
	lda	#5
	eor	zp,x
	cmp	#6
	beq	$+5
	jmp	fail

; eor abs,y

	lda	#3
	sta	mem+4
	ldy	#4
	lda	#5
	eor	mem,y
	cmp	#6
	beq	$+5
	jmp	fail

; eor abs,x

	lda	#3
	sta	mem+5
	ldx	#5
	lda	#5
	eor	mem,x
	cmp	#6
	beq	$+5
	jmp	fail
